- void css(string file)
  link(rel='stylesheet', type='text/css', href='styles/#{file}.css')
- void js(string file)
  script(src='/js/#{file}.js')

doctype html
html
  head
    title Dypronics
    - css("semantic.min");
    - css("Chart.min");
  body
    div(class='ui container')
      block body
    - js("jquery-3.1.1.min");
    - js("semantic.min");
    - js("Chart.min");
