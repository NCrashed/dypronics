- void chart(string name, string title, int w, int h, string color, string units)
  div(class="grid-stack-item", gs-w="#{w}", gs-h="#{h}")
    div(class="grid-stack-item-content")
      canvas(id='#{name}', class="sensor-chart")
      script
        | $(document).ready(function(){ lineChart("#{name}", "#{title}", "#{color}", "#{units}"); });
